<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.lzy.pullzoomview"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".app.DemoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".PullListViewActivity"/>
        <activity android:name=".PullGridViewActivity"/>
        <activity android:name=".PullScrollViewActivity"/>
        <activity android:name=".PullWebViewActivity"/>
        <activity android:name=".PullRecyclerViewActivity"/>
        <activity android:name=".PullViewActivity">
        </activity>
        <activity android:name=".PullViewGroupActivity">
        </activity>
    </application>

</manifest>
